#!name=Script Hub: 重写 & 规则集转换
#!desc=https://script.hub
#!author=@小白脸 @xream @chengkongyiban
#!homepage=https://github.com/Script-Hub-Org/Script-Hub/tree/main
#!openurl=https://script.hub/
#!icon=https://gitlab.com/lodepuly/iconlibrary/-/raw/master/Other_icon/120px/RemoteResourceConverter.png
#!select = 启用插件随机图标, 启用, 禁用
#!select = 替换原始插件图标, 启用, 禁用
#!select = 插件随机图标合集, Doraemon(100P),Shin-chan(100P),Weslie-Wolffy(100P),Tom-Jerry(100P),Genshin(160P),Shin-Miya(100P),OnePiece(100P),Stitch(100P),Pokemon(112P),PokemonGif(56P)
#!input = Parser_cache_exp

[General]

force-http-engine-hosts = script.hub, *.script.hub

[Script]

http-request ^https?:\/\/script\.hub\/$ script-path=https://raw.githubusercontent.com/Script-Hub-Org/Script-Hub/main/script-hub.js, tag=Script Hub: 前端

http-request ^https?:\/\/script\.hub\/file\/_start_\/.+type=qx-rewrite script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/QX-Rewrite-Parser.js, requires-body=true, tag=Script Hub: QX 转换 Loon

http-request ^https?:\/\/script\.hub\/file\/_start_\/.+type=loon-plugin script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/Loon-Rewrite-Parser.js, requires-body=true, tag=Script Hub: Loon 插件控制器

http-request ^https?:\/\/script\.hub\/file\/_start_\/.+type=surge-module script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/Surge-Rewrite-Parser.js, requires-body=true, tag=Script Hub: Surge 转换 Loon

http-request ^https?:\/\/script\.hub\/file\/_start_\/.+type=rule-set script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/rule-parser.js, requires-body=true, tag=Script Hub: 规则集转换

http-request ^https?:\/\/script\.hub\/convert\/ script-path=https://raw.githubusercontent.com/Script-Hub-Org/Script-Hub/main/script-converter.js, requires-body=true, tag=Script Hub: 脚本转换

[MITM]

hostname = script.hub, *.script.hub
